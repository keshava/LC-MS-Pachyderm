{
  "pipeline": {
    "name": "MetFragClient"
  },
  "transform": {
    "image": "container-registry.phenomenal-h2020.eu/phnmnl/metfrag-cli-batch:latest",
    "cmd": [ "/bin/bash" ],
    "stdin": [
    "mkdir metfrag_cli_batch_multiple_output_folder_1;",
    "/usr/local/bin/run_metfrag.sh -p /pfs/Msms2MetFrag/metfrag.txt -r metfrag_cli_batch_multiple_output_folder_1 -l /pfs/params/hmdb_2017-07-23.csv -rn true -o /pfs/out/metfragtab.csv"
    ]
  },
  "parallelism_spec": {
    "constant": 1
  },
  "input": {
    "cross": [
      {
        "atom": {
    "repo": "Msms2MetFrag",
    "glob": "/*"
  }
      },
      {
        "atom": {
    "repo": "params",
    "glob": "/"
  }
      }
    ]
  },
  "scale_down_threshold": "5m",
  "max_queue_size": 1
}